{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/app.py"
    },
    {
      "src": "/analyze",
      "dest": "/api/app.py"
    },
    {
      "src": "/download-json",
      "dest": "/api/app.py"
    },
    {
      "src": "/account/(.*)",
      "dest": "/api/app.py"
    },
    {
      "src": "/neo4j/graph",
      "dest": "/api/app.py"
    },
    {
      "src": "/health",
      "dest": "/api/app.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}